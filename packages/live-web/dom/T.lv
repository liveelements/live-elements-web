
component T{
    constructor(text:string){
        super()
        this._tag = 'text'
        T.prototype.__initialize.call(this)
        this.text = text
    }

    get tag(){ return this._tag }

    string text: ''
    on textChanged: () => {
        if ( this.parent && this.parent.dom ){
            DOMElement.patchOuter(this.parent.dom, this.parent)
        }
    }
}
